#include<stdio.h>
int findFibonacci(int n)
{
 if (n <= 0)
 {
 return 0;
 }
 if (n == 1) 
 {
 return 1;
 }
 return findFibonacci(n-1) + findFibonacci(n-2);
}
int main()
{
 int n;
 printf("Enter the number of terms for Fibonacci series: ");
 scanf("%d", &n);
 for (int i = 0; i < n; i++)
 {
 printf("%d ", findFibonacci(i));
 }
 return 0;
}
